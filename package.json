{
  "name": "ast-parser",
  "version": "0.1.0",
  "description": "A comprehensive AST parser library for TypeScript/JavaScript projects with documentation generation capabilities",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false --silent",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src/**/*.ts",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build",
    "dev": "npm run build:watch",
    "docs": "typedoc src/index.ts --out docs/api",
    "pre-push": "node scripts/pre-push.js",
    "prepare": "husky install",
    "ci": "npm run type-check && npm run lint && npm run test:ci"
  },
  "keywords": [
    "ast",
    "typescript",
    "javascript",
    "parser",
    "documentation",
    "analysis",
    "code-analysis",
    "typescript-parser",
    "ast-parser"
  ],
  "author": "Codestate Team",
  "license": "MIT",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "glob": "^10.3.10",
    "ts-morph": "^21.0.1",
    "typescript": "^5.3.0"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typedoc": "^0.25.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/codestate-cs/ast-parser.git"
  },
  "bugs": {
    "url": "https://github.com/codestate-cs/ast-parser/issues"
  },
  "homepage": "https://github.com/codestate-cs/ast-parser#readme"
}
